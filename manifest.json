{
  "name": "Language Comparer for Wikipedia",
  "description": "Compare amount of information across the languages of a Wikipedia article.",
  "version": "0.1",
  "manifest_version": 3,
  "host_permissions": [
    "https://*.wikipedia.org/*"
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
